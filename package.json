{
  "name": "folee-vue-hooks",
  "private": true,
  "description": "A Custom Hooks Library for Vue3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/LeeSamFong/folee-vue-hooks.git"
  },
  "homepage": "https://github.com/LeeSamFong/folee-vue-hooks",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "bootstrap": "yarn --frozen-lockfile && npx lerna bootstrap",
    "test": "jest",
    "lint": "eslint ./packages --ext .js,.ts",
    "clean:lib": "rimraf lib && rimraf es && rimraf dist",
    "update:version": "esno build/update-version.ts"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^7.16.0",
    "esno": "^0.9.1",
    "flush-promises": "^1.0.2",
    "husky": "^7.0.2",
    "jest": "^27.0.6",
    "lerna": "^4.0.0",
    "lint-staged": "^11.1.2",
    "prettier": "2.3.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.56.3",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^27.0.5",
    "typescript": "^4.3.5",
    "vue": "^3.2.0"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "lint-staged": {
    "*.{js,ts}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
